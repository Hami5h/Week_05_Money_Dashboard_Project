<h1>Update a Merchant</h1>

<div class="data-form">
<form method="post" action="/merchants/<%= @merchants.id %>/update">

  <label for="store-name"> Merchant: </label>
  <input
  type="text"
  name="name"
  value="<%= @merchants.name %>"
  id="store-name"
  />

<input class="btn" type="submit" value="Apply">

</form>
</div>
